{
  "name": "xrange",
  "version": "2.0.0-0",
  "description": "Python-esque iterator for number ranges",
  "author": "Dima Parzhitsky <parzhitsky@gmail.com>",
  "repository": "github:parzh/xrange",
  "license": "MIT",
  "keywords": [
    "range",
    "xrange",
    "iterator"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "start": "node --require ts-node/register src",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --build src --verbose --diagnostics",
    "dev": "npm run build -- --watch",
    "prepublishOnly": "npm test && npm run build && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.9.0",
    "jest": "25.2.7",
    "rimraf": "3.0.2",
    "ts-jest": "25.3.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  }
}
